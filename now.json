{
	"name": "planahead",
    "version": 2,
    "alias": [
        "www.planahead.app",
        "planahead.app"
    ],
    "builds": [{ 
        "src": "next/next.config.js", 
        "use": "@now/next"
    }, {
        "src": "express/index.js",
        "use": "@now/node-server"
    }],
    "env": {
        "OPEN_WEATHER_MAP_TOKEN": "@open-weather-map-token"
    },
    "routes": [{
        "src": "/api", "dest": "/express"
    }, {
        "src": "/", "dest": "/next"
    }]
}